{
  "name": "lolbuff",
  "version": "0.0.1",
  "description": "A minimal skeleton for building testable React apps using ES6.",
  "main": "src/main.jsx",
  "scripts": {
    "server": "webpack --config webpack.local.config.js; node server.js",
    "build": "webpack -p --config webpack.production.config.js",
    "test": "PHANTOMJS_BIN=./node_modules/.bin/phantomjs ./node_modules/karma/bin/karma start karma.config.js",
    "test-cross-browser": "./node_modules/karma/bin/karma start karma.cross-browser.config.js",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "clean": "rm -rf build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pheuter/essential-react.git"
  },
  "keywords": [
    "skeleton",
    "template",
    "react",
    "quickstart"
  ],
  "author": "Mark Fayngersh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pheuter/essential-react/issues"
  },
  "homepage": "https://github.com/pheuter/essential-react",
  "devDependencies": {
    "coveralls": "^2.11.2",
    "html-webpack-plugin": "^1.1.0",
    "istanbul": "^0.3.7",
    "istanbul-instrumenter-loader": "^0.1.2",
    "jquery": "^2.1.3",
    "karma": "^0.12.31",
    "karma-coverage": "^0.2.7",
    "karma-mocha": "^0.1.10",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-sauce-launcher": "^0.2.10",
    "karma-sinon-chai": "^0.3.0",
    "karma-webpack": "^1.5.0",
    "metrics-graphics" : "^2.3.0",
    "moment": "^2.9.0",
    "phantomjs": "^1.9.16",
    "react-hot-loader": "^1.2.3",
    "spin": "^0.0.1",
    "webpack-dev-server": "^1.7.0",

    "jsx-loader": "^0.13.1",
    "json-loader": "^0.5.1",
    "imports-loader": "^0.6.3",
    "less-loader": "^2.2.0",
    "style-loader": "^0.9.0",
    "css-loader": "^0.9.1",
    "file-loader": "^0.8.1",
    "url-loader": "^0.5.5"
  },
  "dependencies": {
    "babel-core": "^5.0.8",
    "babel-loader": "^5.0.0",
    "express": "^4.12.3",
    "react": "^0.13.1",
    "react-router": "^0.13.1",
    "webpack": "^1.7.3",
    "bootstrap": "^3.3.4"
  }
